{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/web/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/web/favicon.ico"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/web/index.html"
    },
    {
      "src": ".*",
      "dest": "/web/index.html"
    }
  ]
}